{
  "name": "vstikzit",
  "type": "module",
  "displayName": "TikZiT for VS Code",
  "description": "TikZiT-like GUI for editing tikz files in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vstikzit.buildTikzFigure",
        "title": "Build TikZ figure",
        "category": "TikZiT"
      },
      {
        "command": "vstikzit.toggleEditor",
        "title": "Toggle TikZ Editor",
        "category": "TikZiT"
      }
    ],
    "keybindings": [
      {
        "command": "vstikzit.toggleEditor",
        "key": "ctrl+shift+t",
        "when": "resourceExtname == .tikz"
      }
    ],
    "languages": [
      {
        "id": "latex",
        "aliases": ["LaTeX", "latex"],
        "extensions": [".tikz"],
        "configuration": ""
      }
    ],
    "customEditors": [
      {
        "viewType": "vstikzit.tikzEditor",
        "displayName": "TikZ Editor",
        "selector": [
          {
            "filenamePattern": "*.tikz"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config-name webview",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src",
    "test": "mocha dist/test.js",
    "pretest": "webpack --config-name test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "mocha": "^11.7.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "immutable": "^5.1.3",
    "process": "^0.11.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-split": "^2.0.14"
  }
}
