{
  "name": "vstikzit",
  "type": "module",
  "displayName": "TikZiT for VS Code",
  "description": "TikZiT-like GUI for editing tikz files in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vstikzit.buildCurrentTikzFigure",
        "title": "Build Current TikZ figure",
        "category": "TikZiT"
      },
      {
        "command": "vstikzit.viewCurrentTikzFigure",
        "title": "View Current TikZ figure",
        "category": "TikZiT"
      }
    ],
    "keybindings": [
      {
        "command": "vstikzit.buildCurrentTikzFigure",
        "key": "ctrl+alt+b",
        "when": "resourceExtname == .tikz"
      },
      {
        "command": "vstikzit.viewCurrentTikzFigure",
        "key": "ctrl+alt+v",
        "when": "resourceExtname == .tikz"
      }
    ],
    "languages": [
      {
        "id": "latex",
        "aliases": ["LaTeX", "latex"],
        "extensions": [".tikz"],
        "configuration": ""
      }
    ],
    "customEditors": [
      {
        "viewType": "vstikzit.tikzEditor",
        "displayName": "TikZ Editor",
        "selector": [
          {
            "filenamePattern": "*.tikz"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "scripts": {
    "watch": "vite build --watch",
    "build": "vite build && vite build --mode extension",
    "build-webview": "vite build",
    "build-extension": "vite build --mode extension",
    "build-with-tsc": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "immutable": "^5.1.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/vscode": "^1.103.0",
    "@vitejs/plugin-react": "^5.0.0",
    "chai": "^6.0.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "mocha": "^11.7.2",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.1.4"
  }
}
