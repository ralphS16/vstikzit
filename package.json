{
  "name": "vstikzit",
  "displayName": "TikZiT for VS Code",
  "description": "TikZiT-like GUI for editing tikz files in VS Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./src/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "vstikzit.tikzEditor",
        "displayName": "TikZ Editor",
        "selector": [
          {
            "filenamePattern": "*.tikz"
          }
        ]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config-name webview",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src",
    "vstest": "vscode-test",
    "test": "mocha dist/test.js",
    "pretest": "webpack --config-name test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.32.0",
    "mocha": "^11.7.1",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "immutable": "^5.1.3",
    "monaco-editor": "^0.52.2",
    "process": "^0.11.10",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-split": "^2.0.14"
  }
}
